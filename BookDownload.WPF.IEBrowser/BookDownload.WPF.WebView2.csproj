<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
    <LangVersion>10.0</LangVersion>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>False</UseWindowsForms>
    <ApplicationIcon>Novel10.ico</ApplicationIcon>
    <Platforms>AnyCPU;x64</Platforms>
    <StartupObject></StartupObject>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>False</Optimize>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <Optimize>False</Optimize>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <Optimize>True</Optimize>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <Optimize>True</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\BookDownload.WPF.IEBrowserOCR\MainWindowAnalysis.cs" Link="MainWindowAnalysis.cs" />
    <Compile Include="..\BookDownload.WPF.IEBrowserOCR\MainWindowDataContext.cs" Link="MainWindowDataContext.cs" />
    <Compile Include="..\BookDownload.WPF.IEBrowserOCR\MainWindowEvent.cs" Link="MainWindowEvent.cs" />
    <Compile Include="..\BookDownload.WPF.IEBrowserOCR\MainWindowEventTest.cs" Link="MainWindowEventTest.cs" />
    <Compile Include="..\BookDownload.WPF.IEBrowserOCR\MainWindowFunc.cs" Link="MainWindowFunc.cs" />
    <Compile Include="..\BookDownload.WPF.IEBrowserOCR\MainWindowLoadUrl.cs" Link="MainWindowLoadUrl.cs" />
    <Compile Include="..\BookDownload.WPF.IEBrowserOCR\MainWindowSpeech.cs" Link="MainWindowSpeech.cs" />
    <Compile Include="..\BookDownload.WPF.IEBrowserOCR\MainWindowWebPageLoadComplete.cs" Link="MainWindowWebPageLoadComplete.cs" />
  </ItemGroup>

	<ItemGroup>
		<Content Include="..\BookDownload.WPF.IEBrowserOCR\tessdata\**\*.*">
			<Link>tessdata\%(RecursiveDir)%(FileName)%(Extension)</Link>
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

	<ItemGroup>
    <COMReference Include="SHDocVw">
      <VersionMinor>1</VersionMinor>
      <VersionMajor>1</VersionMajor>
      <Guid>eab22ac0-30c1-11cf-a7eb-0000c05bae0b</Guid>
      <Lcid>0</Lcid>
      <WrapperTool>tlbimp</WrapperTool>
      <Isolated>false</Isolated>
      <EmbedInteropTypes>true</EmbedInteropTypes>
    </COMReference>
    <COMReference Include="MSHTML">
      <WrapperTool>tlbimp</WrapperTool>
      <VersionMinor>0</VersionMinor>
      <VersionMajor>4</VersionMajor>
      <Guid>3050f1c5-98b5-11cf-bb82-00aa00bdce0b</Guid>
      <Lcid>0</Lcid>
      <Isolated>false</Isolated>
      <EmbedInteropTypes>true</EmbedInteropTypes>
    </COMReference>
  </ItemGroup>


  <ItemGroup>
    <Resource Include="Novel10.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.2592.51" />
    <PackageReference Include="NAudio.Lame" Version="2.1.0" />
    <PackageReference Include="System.Speech" Version="8.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
    <None Update="tessdata\chi_sim.traineddata">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="tessdata\chi_sim_vert.traineddata">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="tessdata\chi_tra.traineddata">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="tessdata\chi_tra_vert.traineddata">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="tessdata\eng.traineddata">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="tessdata\jpn.traineddata">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="tessdata\jpn_vert.traineddata">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="packages\" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Auto3.png" />
    <Resource Include="Auto3.webp" />
    <Resource Include="Auto4.webp" />
    <Resource Include="Auto5.png" />
    <Resource Include="Auto7.png" />
    <Resource Include="Auto8.jpg" />
    <Resource Include="chrome1.jpg" />
    <Resource Include="chrome4.png" />
    <Resource Include="chromium-256.ico" />
    <Resource Include="Novel9.ico" />
    <Resource Include="novel_3_TZV_icon.ico" />
    <Resource Include="refresh4.jpg" />
  </ItemGroup>

</Project>
