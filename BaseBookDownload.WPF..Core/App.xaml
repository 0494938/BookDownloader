<Application x:Class="BookDownloaderWpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converter="clr-namespace:BookDownloaderWpf.Converter"
             StartupUri="WindowsWPFChrome.xaml">
    <Application.Resources>
        <converter:TitleConverter x:Key="TitleConverter"/>
        <converter:EnvironmentConverter x:Key="EnvironmentConverter" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    </Application.Resources>
</Application>
